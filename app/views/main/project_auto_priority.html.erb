		<h1>
			Auto Priority for Tasks:
		</h1>
		<hr>
		<div id="accordion">
			<% @tsk.each_pair do |assignee_id, tasks| %>
			<% u = User.find(assignee_id.to_i)%>
			<h3><a href="#"><%= u.fullname %>: <%= tasks.count%> tasks listed</a></h3>
			<div id="product-list">
				<table>
				<%= render :partial => "task_header"%>
				<%= render :partial => "task" , :collection => tasks, :locals => {:assignee_id => assignee_id}%>
				</table>
			</div>
			<% end %>
			
			</div>